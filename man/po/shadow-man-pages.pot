msgstr ""

#: sg.1.xml:66(refpurpose)
msgid "execute command as different group ID"
msgstr ""

#: sg.1.xml:72(arg) newgrp.1.xml:71(arg)
msgid "-"
msgstr ""

#: sg.1.xml:74(arg)
msgid "-c"
msgstr ""

#: sg.1.xml:73(arg)
